local url = "https://raw.githubusercontent.com/adoniroscratchclub/YZHUB-SHOT-GAME-SCRIPT/refs/heads/main/README.md"

-- Função para carregar o script do link
local function loadFromUrl(link)
    local HttpService = game:GetService("HttpService")
    if not HttpService.HttpEnabled then
        warn("HttpService está desabilitado!")
        return
    end

    local success, result = pcall(function()
        return HttpService:GetAsync(link)
    end)

    if success then
        local func, err = loadstring(result)
        if func then
            func()
        else
            warn("Erro no loadstring: "..err)
        end
    else
        warn("Erro ao pegar o script: "..result)
    end
end

loadFromUrl(url)
